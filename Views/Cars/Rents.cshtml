@model List<Rent_A_Car.Models.Kira>
<!DOCTYPE html>

<html>
<head>
    <title>Rents</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">


</head>
<body>
    <div>

        <br />
        <h2>List of Rents</h2>

        <a class="btn btn-primary btn-sm" href="/">Back</a>
        <a class="btn btn-primary btn-sm" href="/Cars/RentsCreate">New Rents</a>



        <br />
        <br />


        <table class="table table-striped" border="2">
            <thead>
                <tr>
                    @*<th>ID</th>*@
                    <th>Müşteri TC</th>
                    <th>Plaka</th>
                    <th>Tarih</th>
                    <th>Ucret</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var kira in Model)
                {
                    <tr>
                        <td>@kira.kiraMusteriTc</td>
                        <td>@kira.kiraArabaPlaka</td>
                        <td>@kira.kiraTarih</td>
                        <td>@kira.kiraUcret</td>
                        <td>
                            <a class="btn btn-primary btn-sm" href="/Cars/CustomersEdit/@kira.kiraTarih">Edit</a>
                            <a class="btn btn-danger btn-sm" href="/Cars/CustomersDelete/@kira.kiraTarih">Delete</a>

                        </td>
                    </tr>
                }

            </tbody>
        </table>

    </div>

    <!-- Visual Studio Browser Link -->
    <script type="text/javascript" src="/_vs/browserLink" async="async" id="__browserLink_initializationData" data-requestId="48539a83348b4c7ba3aced1e640c564b" data-requestMappingFromServer="false" data-connectUrl="http://localhost:49697/216092f892ea4cc39ef6d8aa3d1a969e/browserLink"></script>
    <!-- End Browser Link -->

    <script src="/_framework/aspnetcore-browser-refresh.js"></script>
</body>
</html>